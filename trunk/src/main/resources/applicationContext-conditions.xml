<!--suppress XmlUnusedNamespaceDeclaration -->
<beans default-lazy-init="true"
       xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
       http://www.springframework.org/schema/aop
       http://www.springframework.org/schema/aop/spring-aop-2.0.xsd
       http://www.springframework.org/schema/tx
       http://www.springframework.org/schema/tx/spring-tx-3.1.xsd"
       xmlns:aop="http://www.springframework.org/schema/aop"
       xmlns:tx="http://www.springframework.org/schema/tx">
    <bean id="baseCondition" class="cn.edu.uestc.acmicpc.db.condition.base.BaseCondition"
          abstract="true" scope="prototype">
    </bean>
    <bean id="userCondition" class="cn.edu.uestc.acmicpc.db.condition.impl.UserCondition"
          parent="baseCondition">
    </bean>
    <bean id="problemCondition" class="cn.edu.uestc.acmicpc.db.condition.impl.ProblemCondition"
          parent="baseCondition">
    </bean>
    <bean id="statusCondition" class="cn.edu.uestc.acmicpc.db.condition.impl.StatusCondition"
          parent="baseCondition">
    </bean>
    <bean id="contestCondition" class="cn.edu.uestc.acmicpc.db.condition.impl.ContestCondition"
          parent="baseCondition">
    </bean>
    <bean id="articleCondition" class="cn.edu.uestc.acmicpc.db.condition.impl.ArticleCondition"
          parent="baseCondition">
    </bean>
    <bean id="trainingUserCondition" class="cn.edu.uestc.acmicpc.db.condition.impl.TrainingUserCondition"
          parent="baseCondition">
    </bean>
</beans>
