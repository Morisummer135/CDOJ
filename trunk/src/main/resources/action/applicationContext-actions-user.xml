<beans default-lazy-init="true"
       xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans-3.1.xsd"
        >


    <bean id="registerAction"
          class="cn.edu.uestc.acmicpc.oj.action.user.RegisterAction"
          parent="baseAction"
          scope="prototype">
        <property name="departmentDAO" ref="departmentDAO"/>
    </bean>
    <bean id="loginAction"
          class="cn.edu.uestc.acmicpc.oj.action.user.LoginAction"
          parent="baseAction"
          scope="prototype">
        <property name="statusCondition" ref="statusCondition"/>
        <property name="statusDAO" ref="statusDAO"/>
    </bean>
    <bean id="logoutAction"
          class="cn.edu.uestc.acmicpc.oj.action.user.LogoutAction"
          parent="baseAction"
          scope="prototype">
    </bean>
    <bean id="userCenterAction"
          class="cn.edu.uestc.acmicpc.oj.action.user.UserCenterAction"
          parent="baseAction"
          scope="prototype">
    </bean>

</beans>